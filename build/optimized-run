#!/bin/bash
ant -f build/build-appengine.xml step-datanucleus-enhance
ant -f build/build-appengine.xml step-stage-prepare
# Assume ${temp.dir} in build.xml file set to /tmp/webapp, in ${user.dir}/local.build.properties
~/Tools/appengine-java-sdk-1.3.6/bin/dev_appserver.sh --port=9999 /tmp/webapp/appengine-stage/
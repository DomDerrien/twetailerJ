# Application global settings
#
# Information loaded automatically by the singleton:
#     twetailer.validator.ApplicationSettings
#

# Boolean used in the JSP file to decide if the Dojo library
# (JavaScript, CSS, images) should be used from a distributed Content
# Delivery Network -- Enabling value: true -- Default: false
useCDN = true

# URL for the Dojo library hosted on a public Content Delivery Network
# Example: http://ajax.googleapis.com/ajax/libs/dojo/1.3
cdnBaseURL = http://ajax.googleapis.com/ajax/libs/dojo/1.4

# Optional relative path from the application context root to the
# login page -- Default: /console/login.jsp
loginPageURL = /jsp_includes/login.jsp

# Optional relative path from the application context root to the
# main page -- Default: /console/index.jsp
mainPageURL = /console/

# Product email address
# Default: maezel@twetailer.appspotmail.com
productEmail = @rwa.productEmail@

# Product name
productName = @rwa.productName@

# Product website
# Default: http://www.twetailer.com/
productWebsite = @rwa.productWebsite@

# Optional URL of the product logo
# Default: /images/logo/logo-48x48.png
# logoURL =

# Relative path of the servlet handling API calls
# Maintained in sync with the information located in the web.xml file by the build process
servletApiPath = @rwa.servletApiPath@

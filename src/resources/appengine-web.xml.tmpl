<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>@rwa.productShortName@</application>
    <version>maezel-@rwa.release@</version>

    <!-- Configure java.util.logging -->
    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
        <property name="myapp.notify-every-n-signups" value="1000" />
        <property name="myapp.notify-url" value="http://www.example.com/signupnotify" />
    </system-properties>

    <env-variables>
    </env-variables>

    <sessions-enabled>true</sessions-enabled>

    <precompilation-enabled>true</precompilation-enabled>

    <ssl-enabled>true</ssl-enabled>

    <inbound-services>
        <service>mail</service>
        <service>xmpp_message</service>
    </inbound-services>

    <admin-console>
        <page name="App Statistics" url="/admin/stats/stats" />
    </admin-console>
</appengine-web-app>
